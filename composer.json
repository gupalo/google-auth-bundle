{
    "name": "gupalo/google-auth-bundle",
    "description": "Google Auth Bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Gupalo\\GoogleAuthBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gupalo\\GoogleAuthBundle\\Tests\\": "Tests/"
        }
    },
    "require": {
        "php": ">=7.4",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/orm": "^2.7|^3.0",
        "gupalo/browser-notifier": "^1.1",
        "gupalo/uidgenerator": "^1.1",
        "knpuniversity/oauth2-client-bundle": "^2.1",
        "league/oauth2-google": "^3.0",
        "symfony/form": "^5.1",
        "symfony/security-core": "^5.1",
        "symfony/security-guard": "^5.1",
        "symfony/translation": "^5.1",
        "symfony/yaml": "^5.0",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.4"
        },
        "sort-packages": true
    }
}
